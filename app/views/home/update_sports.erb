        <!-- Projects Row -->
        <!--<div class="row">-->
 
              </div>
                <form action="/real_sports_update/<%=@pictures.id%>" method="post" enctype="multipart/form-data">
                  <div class="row">
                  <div class="col-md-8 col-sm-4">
                  <div class="form-group">
                    <label for="exampleInputEmail1">제목</label>
                    <input type="text" name="title" class="form-control" id="exampleInputEmail1" placeholder="제목" value="<%= @pictures.title%>">
                  </div>
                  </div>
                  <div class="col-md-4 col-sm-4">
                  <div class="form-group">
                    <label for="exampleInputEmail1">카카오톡 아이디</label>
                    <input type="text" name="kakao" class="form-control" id="exampleInputEmail1" placeholder="카톡아이디" value="<%= @pictures.kakao%>">
                  </div>
                  </div>
                  </div>
                  
                  <div class="row">
                  <div class="col-md-4 col-sm-4">
                  <div class="form-group">
                    <label for="exampleInputEmail2">날짜/시간</label>
                    <input type="text" name="date" class="form-control" id="exampleInputEmail2" placeholder="예)2016.09.01 18:00" value="<%= @pictures.date%>">
                  </div>
                  </div>
                  <div class="col-md-4 col-sm-4">
                   <div class="form-group">
                    <label for="exampleInputEmail2">작성자</label>
                    <input type="text" name="name" class="form-control" id="exampleInputEmail2" placeholder="홍길동" value="<%= @pictures.name%>">
                  </div>
                  </div>
                
                    <input type="hidden" name="current" class="form-control" id="exampleInputEmail2" placeholder="홍길동" value="<%= current_model.email %>">
   
                  
                  <div class="col-md-4 col-sm-4">
                   <div class="form-group">
                    <label for="exampleInputEmail2">장소</label>
                    <input type="text" name="site" class="form-control" id="exampleInputEmail2" placeholder="예)학관 소극장" value="<%= @pictures.site%>">
                  </div>
                  </div>
                  </div>
                  <div>
                    <label for="exampleInputEmail1">내용</label>
                    <textarea class="form-control" name="content" rows="5" id="comment"><%= @pictures.content%>"</textarea>
                  </div>
                    <label class="checkbox-inline">
                      <input name ="what_sports" type="checkbox" id="inlineCheckbox1" value="soccer"> 축구
                    </label>
                    <label class="checkbox-inline">
                      <input name="what_sports" type="checkbox" id="inlineCheckbox2" value="swim"> 수영
                    </label>
                    <label class="checkbox-inline">
                      <input name="what_sports" type="checkbox" id="inlineCheckbox3" value="basket"> 농구
                  
                    </label>
                  <input type="file" name="pic" accept="image/*">
                  <!--<input type="hidden" name="current" value="hihi">-->
                  <button type="submit" class="btn btn-default" style="margin-top:20px;">전송</button>
                </form>
          
